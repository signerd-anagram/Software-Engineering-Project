    ns.getCampProxyActionUrl = getCampProxyActionUrl;
    ns.getCampProxyJsonUrl = getCampProxyJsonUrl;
    ns.urlEncode = urlEncode;

    var PREFIX_APP = "/app/";
    var PREFIX_WIZARDS = "wizards/";
    var EXTENSION_JSON = "json";

    function getViewUrl(controller, action, id) {
        return getUrl(controller, action, null, id);
    }

    function getJsonUrl(controller, action) {
        return getUrl(controller, action, EXTENSION_JSON, null);
    }

    function getWizardUrl(wizardId) {
        return getUrl(PREFIX_WIZARDS + wizardId, null, null, null);
    }

    function getUrl(controller, action, extension, id) {
        var url = PREFIX_APP + controller;

        if (action && action != "") {
            url += "/" + action;
        }

        if (extension && extension != "") {
            url += "." + extension;

        } else if (id && id != '') {
            url += "/" + id
        }

        return url;
    }

    function param(key, value) {
        return key + "=" + value;
    }

    function appendParam(url, key, value) {
        var symbol = Infusion.StringUtils.contains(url, "?") ? "&" : "?";

        return url + symbol + param(key, value);
    }

    function checkForAndCreateHttpIfNoneExists(url) {
        var lowercaseUrl = url.toLowerCase();
        var prefix = lowercaseUrl.substring(0, 8);

        if (prefix.indexOf("http://") == -1 && prefix.indexOf("https://") == -1) {
            lowercaseUrl = "https://" + lowercaseUrl;
        }

        return lowercaseUrl;
    }

    function parseQueryString(url) {
        var nvpair = {};
        var qs = url.substring(url.indexOf('?') + 1);
        var pairs = qs.split('&');

        jQuery.each(pairs, function(indexInArray, valueOfElement) {
            var pair = valueOfElement.split('=');
            nvpair[pair[0]] = pair[1];
        });

        return nvpair;
    }

    function getUrlParts(url) {
        var a = document.createElement('a');
        a.href = url;

        return {
            href: a.href,
            host: a.host,
            hostname: a.hostname,
            port: a.port,
            pathname: a.pathname,
            protocol: a.protocol,
            hash: a.hash,
            search: a.search
        };
    }

    function getCampProxyViewUrl(controller, action, queryString) {
        var url = getViewUrl("campProxy", "view");

        return appendCampProxyParams(url, controller, action, queryString);
    }

    function getCampProxyActionUrl(controller, action) {
        var url = getViewUrl("campProxy", "action");

        return appendCampProxyParams(url, controller, action);
    }

    function getCampProxyJsonUrl(controller, action) {
        var url = getJsonUrl("campProxy", "jsonPost");

        return appendCampProxyParams(url, controller, action);
    }

    function appendCampProxyParams(url, controller, action, queryString) {
        url = appendParam(url, "controller", controller);
        url = appendParam(url, "action", action);

        if (queryString) {
            url = appendParam(url, "queryString", urlEncode(queryString));
        }

        return url;
    }

    function urlEncode(url) {
        return encodeURIComponent(url).replace(/%20/g,'+');
    }

