                    ret = str + "s";

                } else {
                    ret = str.substring(0, str.length - 1) + "ies";
                }

            } else {
                ret = str + "s";
            }
        }

        return ret;
    }

    function getLastCharacter(str) {
        return str.charAt(str.length - 1);
    }

    function isVowel(letter) {
        return letter == 'a' || letter == 'e' || letter == 'i' || letter == 'o' || letter == 'u' ||
